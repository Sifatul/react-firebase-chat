{
  "name": "react-firebase-chat",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./lib/cjs/FirebaseModule/index.d.ts",
  "dependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/aria-query": "^4.2.1",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.20.13",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-icons": "^4.2.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.3.2",
    "uuid": "^8.3.2",
    "web-vitals": "^1.1.2"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "npm build:esm && npm build:cjs",
    "build:esm": "tsc --noEmit false",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:cjs": "tsc --noEmit false --module commonjs --outDir lib/cjs",
    "predeploy": "rm -rf dist && NODE_ENV=production babel lib/cjs/FirebaseModule/ --out-dir dist --copy-files",
    "build:webpack": "webpack --mode production --progress "
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.14.5",
    "@types/uuid": "^8.3.0",
    "firebase": "^8.6.3",
    "install": "^0.13.0",
    "npm": "^7.15.0",
    "rollup": "^2.51.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.30.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
